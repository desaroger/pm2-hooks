{
  "extends": ["airbnb"],
  "rules": {
    "max-len": [1, 120, 2, {"ignoreComments": true}],
    "quote-props": [1, "consistent-as-needed"],
    "no-cond-assign": [2, "except-parens"],
    "radix": 0,
    "space-infix-ops": 0,
    "no-unused-vars": [1, {"vars": "local", "args": "none"}],
    "default-case": 0,
    "no-else-return": 0,
    "no-param-reassign": 0,
    "quotes": 0,
    "indent": ["error", 4],
    "comma-dangle": [1, 0],
    "consistent-return": [0],
    "no-underscore-dangle": [0],
    "class-methods-use-this": 0,
    "no-use-before-define": 0,
    "prefer-const": 0,
    "arrow-body-style": 0,
    "no-unused-expressions": 0,
    "func-names": 0,
    "no-plusplus": 0,
    "no-console": 0
  },
  "env": {
    "node": true,
    "mocha": true,
    "jasmine": true
  }
}